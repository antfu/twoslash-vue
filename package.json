{
  "name": "twoslash-vue",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@8.14.1",
  "description": "Extended TwoSlash that supports Vue SFC",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/antfu/twoslash-vue#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/twoslash-vue.git"
  },
  "bugs": "https://github.com/antfu/twoslash-vue/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "typescript": "*"
  },
  "dependencies": {
    "@vue/language-core": "^1.8.27",
    "twoslashes": "^0.0.4",
    "vue": "^3.4.11"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.2",
    "@antfu/ni": "^0.21.12",
    "@antfu/utils": "^0.7.7",
    "@types/node": "^20.11.0",
    "bumpp": "^9.2.1",
    "eslint": "^8.56.0",
    "esno": "^4.0.0",
    "lint-staged": "^15.2.0",
    "pnpm": "^8.14.1",
    "rimraf": "^5.0.5",
    "shikiji": "^0.9.18",
    "shikiji-twoslash": "^0.9.18",
    "simple-git-hooks": "^2.9.0",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0",
    "vite": "^5.0.11",
    "vitest": "^1.1.3"
  },
  "pnpm": {
    "patchedDependencies": {
      "shikiji-twoslash@0.9.18": "patches/shikiji-twoslash@0.9.18.patch"
    }
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
